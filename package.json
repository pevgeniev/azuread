{
  "name": "devocean-demo",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:github": "ng build --output-path docs --base-href /azuread/",
    "build:prod": "ng build --configuration production",
    "build:stage": "ng build --configuration stage",
    "postinstall:msal-angular": "node ../sample-scripts/useMsalAngularTarball.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "test:e2e": "jest",
    "generate:service": "openapi-generator-cli generate -i http://localhost:5120/swagger/v1/swagger.json -g typescript-angular --type-mappings=DateTime=Date -o ./src/app/backend/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@azure/msal-angular": "^4.0.0",
    "@azure/msal-browser": "^4.0.0",
    "@microsoft/signalr": "^8.0.7",
    "@types/luxon": "^3.6.2",
    "dayjs": "^1.11.13",
    "font-awesome": "^4.7.0",
    "luxon": "^3.6.1",
    "ngx-mat-select-search": "^8.0.1",
    "powerbi-client-angular": "^5.0.0",
    "rxjs": "~7.8.0",
    "tabulator-tables": "^6.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.0",
    "@angular/cli": "^19.1.0",
    "@angular/compiler-cli": "^19.1.0",
    "@openapitools/openapi-generator-cli": "^2.16.3",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@types/tabulator-tables": "^6.2.6",
    "e2e-test-utils": "file:../../e2eTestUtils",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "schematics-scss-migrate": "^2.3.17",
    "ts-jest": "^29.1.2",
    "typescript": "~5.7.3"
